{
  "manifest_version": 3,

  "name": "My Extension",
  "description": "This extension is a starting point to create a real Chrome extension",
  "version": "0.0.1",

  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "chrome_url_overrides": {
    "bookmarks": "index.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo192.png",
      "32": "logo192.png",
      "48": "logo192.png",
      "128": "logo192.png"
    }
  },
  "permissions": ["bookmarks", "storage", "contextMenus"]
}
